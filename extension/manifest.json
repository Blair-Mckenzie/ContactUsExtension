{
    "name": "Make Those Contact Us Buttons Better",
    "description": "Zoom zoom",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ],
    "permissions": [
        "scripting"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}